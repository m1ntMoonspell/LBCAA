{
    // 本文件主要定义了通用 UI 的行为和显示内容
    // 可参考文档和示例添加更多您需要的功能
    // 文档：https://maafw.xyz/docs/3.3-ProjectInterfaceV2
    // 示例：https://github.com/MaaXYZ/MaaFramework/blob/main/sample/interface.json
    "interface_version": 2,
    "name": "LCBAA",
    "description": "This is auto assistant for LimbusCompany based on MaaFramework",
    "contact": "<a href>'https://github.com/m1ntMoonspell?tab=repositories'</a>",
    "license": "MIT",
    "welcome": "首次使用！欢迎使用 LCBAA!",
    // 填写项目地址和版本号可实现自动更新功能
    "github": "https://github.com/m1ntMoonspell/LBCAA",
    "version": "1.0.0",
    // Mirror酱为第三方分发服务，新项目默认关闭
    // 若有需要请联系 Mirror酱 开通，直接自行开启是无法使用的
    // "mirrorchyan_rid": "MaaXXX",
    // "mirrorchyan_multiplatform": true,
    "controller": [
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": "UnityWndClass",
                "window_regex": "LimbusCompany",
                // 截图及控制方式，请根据项目实际情况修改
                // 文档: https://maafw.xyz/docs/2.4-ControlMethods
                "screencap": "PrintWindow", // 截图方式
                "mouse": "PostMessageWithCursorPos", // 鼠标控制方式
                "keyboard": "PostMessageWithCursorPos" // 键盘控制方式
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "./resource"
            ]
        }
    ],
    // 可选的 agent 示例
    //"agent": {
    //    "child_exec": "python",
    //   "child_args": [
    //        "-u",
    //        "./../agent/main.py"
    //    ]
    //},
    "task": [
        {
            "name": "领取日常奖励",
            "entry": "ClaimDailyRewards",
            "pipeline_override": {
                "BPSceneReco": {
                    "template": "Season7BPScene.png"
                },
                "ClickSeasonBanner": {
                    "template": "Season7Banner.png"
                }
            }
        }
    ],
    "option": {
        "任务选项1": {
            "cases": [
                {
                    "name": "文本1",
                    "pipeline_override": {
                        "OcrTask": {
                            "text": "文本1"
                        }
                    }
                },
                {
                    "name": "文本2",
                    "pipeline_override": {
                        "OcrTask": {
                            "text": "文本2"
                        }
                    }
                }
            ]
        }
    }
}